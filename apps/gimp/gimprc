# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(tile-cache-size 4040903k)
(default-image
    (width 2480)
    (height 3508)
    (unit millimeters)
    (xresolution 300)
    (yresolution 300)
    (resolution-unit inches)
    (image-type rgb)
    (precision u8-gamma)
    (color-managed yes)
    (color-profile NULL)
    (fill-type background)
    (comment "Created with GIMP"))
(undo-levels 50)
(undo-size 1010229k)
(thumbnail-size large)
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(transparency-size large-checks)
(transparency-type light-checks)
(monitor-xresolution 124)
(monitor-yresolution 122)
(navigation-preview-size small)
(move-tool-changes-active yes)
(toolbox-wilber no)
(theme "System")
(icon-theme "Symbolic-Inverted")
(image-convert-indexed-palette-type web)
(layer-new-name "recolorb")
(layer-add-mask-type selection)
(selection-grow-radius 2)
(selection-shrink-radius 2)
(selection-border-radius 1)
(selection-border-edge-lock yes)
(fill-options
    (style solid)
    (antialias no)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10)
    (method line)
    (width 8)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))


# end of gimprc
